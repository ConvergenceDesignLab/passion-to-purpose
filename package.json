{
  "name": "cdl-passion-to-purpose",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.3",
    "node-sass-chokidar": "^1.3.0",
    "prettier": "^1.13.5",
    "react": "^16.4.1",
    "react-app-rewire-mobx": "^1.0.8",
    "react-app-rewired": "^1.5.2",
    "react-dom": "^16.4.1",
    "react-ga": "^2.5.3",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "react-transition-group": "^2.3.1",
    "store": "^2.0.12"
  },
  "prettier": {
    "printWidth": 100
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css -- --watch",
    "start": "set \"NODE_ENV=development\" && concurrently \"npm run watch-css\" \"react-app-rewired start\"",
    "react:dev": "set \"NODE_ENV=development\" && react-app-rewired start",
    "build": "npm run build css && react-app-rewired build",
    "test": "npm run build css && react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
